{
  "name": "apifetch-json",
  "version": "2.1.1",
  "description": "ApiFetch is a fetch() wrapper that provides an easy to read API for reading and writing JSON objects over HTTP(S) and REST apis",
  "main": "dist/lib/commonjs/index.js",
  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/apifetch.git"
  },
  "keywords": [
    "fetch()",
    "fetch",
    "api",
    "json",
    "serialize",
    "deserlize",
    "REST",
    "restful"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scottbamford/apifetch/issues"
  },
  "homepage": "https://github.com/scottbamford/apifetch/#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && tsc -m esnext --outDir ./dist/lib/esm",
    "rebuild": "npm run-script clean && npm run-script build"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "typescript": "3.4.5",
    "rimraf": "^2.6.3"
  }
}
